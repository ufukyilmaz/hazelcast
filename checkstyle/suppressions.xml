<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>

    <!-- Suppress duplicate checking of copyright notice -->
    <suppress checks="StrictDuplicateCode" files=".java" lines="1-15"/>

    <!-- TODO: these wildcard suppressions need to be fixed -->
    <suppress checks="" files="com/hazelcast/security/"/>
    <suppress checks="" files="com/hazelcast/memory/"/>
    <suppress checks="" files="com/hazelcast/elastic/"/>

    <!-- Elastic Memory -->
    <suppress checks="JavadocMethod" files="com/hazelcast/elasticmemory/"/>
    <suppress checks="JavadocType" files="com/hazelcast/elasticmemory/"/>
    <suppress checks="ExecutableStatementCount|NPathComplexity" files="com/hazelcast/elasticmemory/ByteBufferStorage"/>
    <suppress checks="IllegalImport" files="com/hazelcast/elasticmemory/UnsafeStorage"/>

    <!-- NIO -->
    <suppress checks="JavadocMethod" files="com/hazelcast/nio/"/>
    <suppress checks="JavadocType" files="com/hazelcast/nio/"/>
    <suppress checks="JavadocMethod" files="com/hazelcast/nio/serialization/"/>
    <suppress checks="JavadocType" files="com/hazelcast/nio/serialization/"/>
    <suppress checks="JavadocMethod" files="com/hazelcast/internal/serialization/"/>
    <suppress checks="JavadocType" files="com/hazelcast/internal/serialization/"/>
    <suppress checks="ParameterNumber" files="com/hazelcast/internal/serialization/impl/EnterpriseSerializationServiceImpl"/>
    <suppress checks="MethodCount|MagicNumber" files="com/hazelcast/internal/serialization/impl/MemoryBlockDataInput"/>
    <suppress checks="IllegalImport|NPathComplexity" files="com/hazelcast/internal/serialization/impl/NativeMemoryDataUtil"/>
    <suppress checks="MagicNumber" files="com/hazelcast/nio/CipherHelper"/>
    <suppress checks="NPathComplexity" files="com/hazelcast/nio/tcp/SymmetricCipherPacketReader"/>
    <suppress checks="NPathComplexity" files="com/hazelcast/nio/tcp/SymmetricCipherPacketWriter"/>

    <!-- Instance -->
    <suppress checks="ClassFanOutComplexityCheck" files="com/hazelcast/instance/EnterpriseNodeExtension"/>

    <!-- Sessions -->
    <suppress checks="JavadocMethod" files="com/hazelcast/session/"/>
    <suppress checks="JavadocType" files="com/hazelcast/session/"/>
    <suppress checks="JavadocVariable" files="com/hazelcast/session/"/>
    <suppress checks="MethodCountCheck" files="com/hazelcast/session/HazelcastSessionManager"/>

    <!-- SSL -->
    <suppress checks="MagicNumber|MethodLength|MagicNumber|CyclomaticComplexity|EmptyBlock|NPathComplexity"
              files="com/hazelcast/nio/ssl/SSLSocketChannelWrapper"/>

    <suppress checks="NPathComplexity" files="com/hazelcast/nio/tcp/SymmetricCipherMemberWriteHandler"/>
    <suppress checks="NPathComplexity" files="com/hazelcast/nio/tcp/SymmetricCipherMemberReadHandler"/>

    <!-- HiDensity -->
    <suppress checks="MethodCountCheck|NPathComplexity|CyclomaticComplexity"
              files="com/hazelcast/cache/hidensity/impl/nativememory/HiDensityNativeMemoryCacheRecordStore"/>
    <suppress checks="NPathComplexity"
              files="com/hazelcast/cache/hidensity/nearcache/impl/nativememory/HiDensityNativeMemoryNearCacheRecordStore"/>
    <suppress checks="ClassDataAbstractionCouplingC"
              files="com/hazelcast/cache/hidensity/operation/HiDensityCacheDataSerializerHook"/>
    <suppress checks="ClassDataAbstractionCouplingC"
              files="com/hazelcast/cache/hidensity/operation/HiDensityCacheOperationProvider"/>

    <!-- Enterprise Cache -->
    <suppress checks="ClassDataAbstractionCouplingC|ClassFanOutComplexityCheck"
              files="com/hazelcast/cache/EnterpriseCacheService"/>

    <!-- Hot Restart module -->
    <suppress checks="InnerAssignment|VisibilityModifier" files="com/hazelcast/spi/hotrestart/impl/gc/"/>
    <suppress checks="VisibilityModifier|MagicNumber" files="com/hazelcast/spi/hotrestart/impl/HeavyKey"/>
    <suppress checks="ClassData" files="com/hazelcast/spi/hotrestart/impl/HotRestartStoreImpl"/>
    <suppress checks="MagicNumber|Complexity" files="com/hazelcast/spi/hotrestart/impl/lz4"/>
    <suppress checks="OuterTypeNumber|MultipleVariableDeclarations|MagicNumber" files="com/hazelcast/util/concurrent"/>

    <!-- Query Cache -->
    <suppress checks="MethodCountCheck" files="com/hazelcast/map/impl/querycache/subscriber/DefaultQueryCache"/>
    <suppress checks="ParameterNumber" files="com/hazelcast/map/impl/querycache/accumulator/AccumulatorInfo"/>

    <!-- Client Protocol -->
    <suppress checks="" files="com/hazelcast/client/impl/protocol/template/"/>

</suppressions>
